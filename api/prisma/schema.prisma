// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Invoice {
  id                     Int       @id @default(autoincrement())
  number_invoice         String
  serie_invoice          String    @default("")
  number_client          String
  competency             DateTime
  energy_electrical      String
  energy_scee            String
  energy_compensated     String
  municipal_contribution Float
  total_price            Float
  created_at             DateTime? @default(now())

  @@map("invoices")
}
